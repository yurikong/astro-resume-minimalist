---
// Hack: Iconify icons referenced through astro endpoints are not bundled, therefore import theme here for bundle processing.
const iconClassNames: string[] = [
  "i-ri:wechat-line",
  "i-ri:mail-line",
  "i-ri:earth-line",
  "i-ri:github-line",
  "i-ri:github-fill",
  "i-ri:linkedin-box-line",
  "i-ri:instagram-line",
  "i-ri:twitter-x-line",
].toSorted()
---

<div class="hidden">
  {iconClassNames.map((it) => <i class={it} />)}
</div>
